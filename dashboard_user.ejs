<h2>Welcome <%= user.username %></h2>
<a href="/user/add">Tambah Pekerjaan</a>
<ul>
    <% jobs.forEach(job => { %>
        <li>
            <strong><%= job.job_name %></strong> | Mulai: <%= job.start_date %> - Deadline: <%= job.deadline %>
            [<%= job.is_verified ? "✔️ Diverifikasi" : "⏳ Menunggu Verifikasi" %>]
        </li>
    <% }) %>
</ul>
